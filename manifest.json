{
  "manifest_version": 2,
  "name": "My Extension",
  "version": "1.0",
  "description": "This extension loads a web page inside of the extension.",
  "permissions": [
    "activeTab",
	"tabs",
    "storage",
	"webRequest",
	"<all_urls>",
	"webRequestBlocking"
	],
  "browser_action": {
    "default_popup": "popup.html",
	"keep_open": true
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}
