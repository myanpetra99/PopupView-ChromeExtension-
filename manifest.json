{
  "manifest_version": 2,
  "name": "PopUp WebView",
  "version": "1.0",
  "description": "Load a web page inside of the extension.",
  "permissions": [
    "activeTab",
	"tabs",
    "storage",
	"webRequest",
	"<all_urls>",
	"webRequestBlocking"
	],
  "browser_action": {
    "default_popup": "popup.html",
	"keep_open": true
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
   "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
